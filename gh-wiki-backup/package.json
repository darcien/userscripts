{
  "name": "gh-wiki-backup",
  "description": "Automatically backup unsaved GitHub Wiki changes before page unload.",
  "author": "darcien",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch --mode development --outDir dist-dev",
    "build:next": "vite build --outDir dist-next",
    "build:stable": "vite build --outDir dist && pnpm patch-dist",
    "build": "pnpm build:next",
    "format": "deno fmt",
    "patch-dist": "deno run --allow-read --allow-write scripts/patch-dist.ts"
  },
  "devDependencies": {
    "idb-keyval": "^6.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-userscript-plugin": "^1.8.1"
  }
}
